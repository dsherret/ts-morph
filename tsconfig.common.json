{
  "compilerOptions": {
    "target": "es2018",
    "module": "CommonJS",
    "strict": true,
    "sourceMap": false,
    "removeComments": false,
    "experimentalDecorators": true,
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": true,
    "newLine": "lf",
    "importHelpers": false
  },
  "compileOnSave": false
}
